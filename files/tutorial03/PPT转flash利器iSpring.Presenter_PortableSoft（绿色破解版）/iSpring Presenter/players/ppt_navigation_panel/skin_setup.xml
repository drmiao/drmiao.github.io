<?xml version="1.0"?>
<skinSetup>
	<i18n>
		<customizationWindow>
			<title>Navigation panel customization</title>
			<tabs>
				<drawing>
					<title>Drawing</title>
					<enableDrawingMenu>Enable drawing menu</enableDrawingMenu>
					<arrow>Arrow</arrow>
					<pencil>Pencil</pencil>
					<feltPen>Felt Pen</feltPen>
					<highlighter>Highlighter</highlighter>
					<eraser>Eraser</eraser>
					<eraseAll>Erase All</eraseAll>
				</drawing>
				<settings>
					<title>Settings</title>
					<firstSlide>First Slide button</firstSlide>
					<lastSlide>Last Slide button</lastSlide>
					<navigationBy>Navigation by</navigationBy>
					<slide>Slide</slide>
					<step>Step</step>
					<layout>Layout</layout>
				</settings>
				<messages>
					<title>Messages</title>
