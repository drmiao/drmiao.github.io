<!-- 
	This configuration file is used for the 'Classic' skin customization.
	You can modify some parts of this file to change skin appearance and texts in it. Follow the instructions in comments.
	
	Remember to make a backup copy of this file in order to make any changes in it.
	The format of this file can be changed in future versions of FlashSpring without any notification 
	and your changes may become incompatible with the new skin versions.
-->
<publishSettings skinModule="FullFeatured">
	<externalAssets>
		<!-- 
			If you don't want thumbnails to be generated you may need to set the 'enabled' attribute value of the thumbnails tag to 0 
			Do not forget to add <thumbnails>0</thumbnails> line to skin_settings.xml
			Don't change the width and the height of thumbnails in this XML file - these properties are used for internal purposes only
			-->
		<thumbnails enabled="1">
			<size width="120" height="90"/>
		</thumbnails>
		<!--
			Don't change the width and the height of presener photo in this XML file - these properties are used for internal purposes only
		-->
		<presenterPhoto enabled="1">
			<size width="60" height="80"/>
		</presenterPhoto>
	</externalAssets>
	<additionalSize width="224" height="103"/>
	<fonts>
		<font family="Arial" bold="0" italic="0" name="iSpringSkinFont">
			<charsets>
				<staticCharset characters="-1234567890/: "/>
				<!-- These characters are used to display numeric information -->
				<!-- read comments on label properties in skin_settings.xml -->
				<staticCharset characters="Outline"/>
				<staticCharset characters="Thumbnails"/>
				<staticCharset characters="Presenter"/>
				<staticCharset characters="Notes"/>
				<staticCharset characters="Slide: /"/>
				<staticCharset characters="You must complete the quiz before leaving this slide."/>
				<staticCharset characters="Would you like to resume the presentation from the last slide viewed?"/>
				<staticCharset characters="OK"/>
				<staticCharset characters="Yes"/>
				<staticCharset characters="No"/>
				<!-- Do not change the dynamic charset properties - they are used for internal purposes only -->
				<dynamicCharset type="slideTitle"/>
				<dynamicCharset type="presenterEmail"/>
				<dynamicCharset type="presenterInfo"/>
				<dynamicCharset type="presenterWebsite"/>
				<dynamicCharset type="presenterTitle"/>
				<dynamicCharset subset="usingFontStyle" type="slideNotesText"/>
			</charsets>
		</font>
		<font family="Arial" bold="1" italic="0" name="iSpringSkinFont">
			<charsets>
				<staticCharset characters="1234567890/:% "/>
				<staticCharset characters="Warning"/>
				<staticCharset characters="Resume Presentation"/>
				<!-- Do not change the dynamic charset properties - they are used for internal purposes only -->
				<dynamicCharset type="presentationTitle"/>
				<dynamicCharset type="presenterName"/>
				<dynamicCharset subset="usingFontStyle" type="slideNotesText"/>
			</charsets>
		</font>
		<font name="iSpringSkinFont" bold="0" italic="1" family="Arial">
			<charsets>
				<dynamicCharset subset="usingFontStyle" type="slideNotesText"/>
			</charsets>
		</font>
		<font name="iSpringSkinFont" bold="1" italic="1" family="Arial">
			<charsets>
				<dynamicCharset subset="usingFontStyle" type="slideNotesText"/>
			</charsets>
		</font>
	</fonts>
	<texts>
		<!-- The following properties are used internally - don't change them' -->
		<staticText type="slideTitle"/>
		<staticText type="slideNotesText"/>
		<staticText type="presenterEmail"/>
		<staticText type="presenterInfo"/>
		<staticText type="presenterWebsite"/>
		<staticText type="presenterTitle"/>
		<staticText type="presentationTitle"/>
		<staticText type="presenterName"/>
		<staticText type="slideNotesHtml"/>
	</texts>
</publishSettings>
